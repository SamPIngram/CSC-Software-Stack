{
    "version": "2",
    "templates": [
      {
        "categories": [
          "Python", "Development"
        ],
        "description": "Python Dev is VS Code running on a remote server, accessible through the browser.",
        "env": [
          {
            "name": "PYTHON_VERSION",
            "label": "PYTHON_VERSION",
            "select": [
              {
                "text": "Python 3.11",
                "value": "3.11",
                "default": true
              },
              {
                "text": "Python 3.9",
                "value": "3.9"
              },
              {
                "text": "Python 3.8",
                "value": "3.8"
              },
              {
                "text": "Python 3.7",
                "value": "3.7"
              }
            ]
          },
          {
            "name": "NUM_GPU",
            "label": "NUM_GPU",
            "select": [
              {
                "text": "0",
                "value": "0",
                "default": true
              },
              {
                "text": "1",
                "value": "1"
              },
              {
                "text": "2",
                "value": "2"
              }
            ]
          },
          {
            "label": "PASSWORD",
            "name": "PASSWORD"
          },
          {
            "label": "SUDO_PASSWORD",
            "name": "SUDO_PASSWORD"
          }
        ],
        "repository": {
          "url": "https://github.com/SamPIngram/CSC-Software-Stack",
          "stackfile": "applications/python-dev/docker-compose.yml"
        },
        "logo": "https://raw.githubusercontent.com/Qballjos/portainer_templates/master/Images/code-server.png",
        "name": "python-dev",
        "platform": "Linux",
        "restart_policy": "unless-stopped",
        "title": "Python Dev",
        "type": 3,
        "volumes": [
          {
            "bind": "/portainer/Files/AppData/Config/Code-Server",
            "container": "/config"
          },
          {
            "bind": "/etc/ssl/certs:/etc/ssl/certs",
            "container": "/etc/ssl/certs:/etc/ssl/certs"
          }
        ]
      }
    ]
  }
