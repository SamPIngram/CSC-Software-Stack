{
    "version": "2",
    "templates": [
      {
        "categories": [
          "Python", "Development"
        ],
        "description": "Python Dev is VS Code running on a remote server, accessible through the browser.",
        "env": [
          {
            "name": "INSTANCE_ID",
            "label": "INSTANCE_ID"
          },
          {
            "name": "PYTHON_VERSION",
            "label": "PYTHON_VERSION",
            "select": [
              {
                "text": "Python 3.11",
                "value": "3.11",
                "default": true
              },
              {
                "text": "Python 3.9",
                "value": "3.9"
              },
              {
                "text": "Python 3.8",
                "value": "3.8"
              },
              {
                "text": "Python 3.7",
                "value": "3.7"
              },
              {
                "text": "Python 2.7 (not recommended)",
                "value": "2.7"
              }
            ]
          },
          {
            "label": "PASSWORD",
            "name": "PASSWORD"
          },
          {
            "label": "SUDO_PASSWORD",
            "name": "SUDO_PASSWORD"
          }
        ],
        "repository": {
          "url": "https://github.com/SamPIngram/CSC-Software-Stack",
          "stackfile": "applications/python-dev/docker-compose.yml"
        },
        "logo": "https://raw.githubusercontent.com/SamPIngram/CSC-Software-Stack/main/misc/icons/python.png",
        "name": "python-dev",
        "platform": "Linux",
        "restart_policy": "unless-stopped",
        "title": "Python Dev",
        "type": 3
