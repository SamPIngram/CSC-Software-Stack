{
    "version": "2",
    "templates": [
      {
        "categories": [
          "Python", "Development"
        ],
        "description": "Python Dev is VS Code running on a remote server, accessible through the browser.",
        "env": [
          {
            "label": "PASSWORD",
            "name": "PASSWORD"
          },
          {
            "label": "SUDO_PASSWORD",
            "name": "SUDO_PASSWORD"
          }
        ],
        "image": "docker.pkg.github.com/sampingram/csc-python-code-server-3.7:latest",
        "logo": "https://raw.githubusercontent.com/Qballjos/portainer_templates/master/Images/code-server.png",
        "name": "python-dev",
        "platform": "Linux",
        "ports": [
          ":8443/tcp"
        ],
        "restart_policy": "unless-stopped",
        "title": "Python Dev",
        "type": 1,
        "volumes": [
          {
            "bind": "/portainer/Files/AppData/Config/Code-Server",
            "container": "/config"
          }
        ]
      }
    ]
  }
