{
    "RemoteAccessAllowed": true,
    "AuthenticationEnabled": true,
    "PostgreSQL": {
      "EnableIndex": true,
      "EnableStorage": false, // DICOM files are stored in the Orthanc container in /var/lib/orthanc/db/
      "Host": "orthanc-index", // the name of the PostgreSQL container
      "Database": "postgres", // default database name in PostgreSQL container (no need to create it)
      "Username": "postgres", // default user name in PostgreSQL container (no need to create it)
      "Password": "postgres"
    },
  
    "ExtraMainDicomTags": {
      "Instance" : [
          "Rows",
          "Columns",
          "ImageType",
          "SOPClassUID",
          "ContentDate",
          "ContentTime",
          "FrameOfReferenceUID",
          "PixelSpacing",
          "SpecificCharacterSet",
          "BitsAllocated",
          "BitsStored",
  
          "RescaleSlope",
          "RescaleIntercept",
          "SliceThickness",
          "WindowCenter",
          "WindowWidth",
          "PhotometricInterpretation",
          "PixelRepresentation"
      ],
      "Series" : [
          "TimezoneOffsetFromUTC",
          "PerformedProcedureStepStartDate",
          "PerformedProcedureStepStartTime",
          "SpecificCharacterSet",
          "RequestAttributesSequence"
      ],
      "Study": [
          "TimezoneOffsetFromUTC",
          "SpecificCharacterSet",
          "PatientComments",
          "IssuerOfPatientID",
          "PatientBirthTime"
      ],
      "Patient": []
    }
  }